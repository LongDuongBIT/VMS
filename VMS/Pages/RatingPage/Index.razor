@attribute [Route(Routes.RatingPage + "/{activityId:int}")]
@attribute [Authorize(Roles = "Organization")]

<div class="rating-page">
    <Title />
    <div class="header-page">
        <div class="search-box">
            <VMS.Pages.ActivitySearchPage.SearchBox />
        </div>
        <FilterBar />
    </div>
    <div class="body-page">
        <MemberList ActivityId=@ActivityId StarRating="starRating" />
        <FilterRatingAll @bind-StarRating="starRating" />
    </div>
</div>

